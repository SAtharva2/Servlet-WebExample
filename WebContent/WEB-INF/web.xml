<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">

  

  
  <context-param>
    <param-name>driver</param-name>
    <param-value>oracle.jdbc.driver.OracleDriver</param-value>
  </context-param>
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:oracle:thin:@adm-PC:1521:XE</param-value>
  </context-param>
  
  
  <servlet>
    <description></description>
    <display-name>DemoServJDBC2</display-name>
    <servlet-name>DemoServJDBC2</servlet-name>
    <servlet-class>com.seed.DemoServJDBC2</servlet-class>
    <init-param>
     <param-name>dbuser</param-name>
     <param-value>hr</param-value>
    </init-param>
    <init-param>
     <param-name>dbpwd</param-name>
     <param-value>hr</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>DemoServJDBC2</servlet-name>
    <url-pattern>/DemoServJDBC2</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>SampleServlet</display-name>
    <servlet-name>SampleServlet</servlet-name>
    <servlet-class>com.seed.SampleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SampleServlet</servlet-name>
    <url-pattern>/SampleServlet</url-pattern>
  </servlet-mapping>
</web-app>


  
  
  
  
  
  








